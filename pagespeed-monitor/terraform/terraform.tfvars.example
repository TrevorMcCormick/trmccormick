aws_region            = "us-east-1"
project_name          = "pagespeed-monitor"
environment           = "prod"
target_url            = "https://trmccormick.com"
github_webhook_secret = "your-random-secret-token-here"  # Generate with: openssl rand -hex 32
pagespeed_api_key     = ""  # Optional: Get from https://console.cloud.google.com/apis/credentials
